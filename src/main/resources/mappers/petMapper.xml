<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zoozooclub.pet.model.mapper.PetMapper">
	<select id="petList" resultType="Pet">
	select *
	from pet
	where MEMBER_NO = #{memberNo}
	</select>
	<insert id="savePet">
	INSERT INTO pet
	values(pet_seq.NEXTVAL, #{petName},#{petCategory}, #{petBirthDay},#{gender},#{petNote},#{memberNo}, #{neutered}, #{petWeight}, #{petImage})
	</insert>
</mapper>